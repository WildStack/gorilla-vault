{
  "name": "gorilla-vault",
  "version": "0.0.1",
  "description": "This is gorilla vault secure storage solution",
  "author": "Giorgi Kumelashvili",
  "private": "false",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "build:run": "vite preview",
    "lint": "npm run use:nvm && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "modules:update:check": "npm run use:nvm && npx npm-check-updates",
    "modules:update": "npm run use:nvm && npx npm-check-updates -u",
    "use:nvm": "source ~/.nvm/nvm.sh && nvm use",
    "install": "npm run use:nvm",
    "preinstall": "npm run use:nvm",
    "prebuild": "npm run use:nvm",
    "preversion": "npm run use:nvm",
    "prestart": "npm run use:nvm"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.10.0",
    "@blueprintjs/datetime2": "^2.3.2",
    "@blueprintjs/select": "^5.1.2",
    "@blueprintjs/table": "^5.1.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "axios": "^1.6.7",
    "class-transformer": "^0.5.1",
    "filenamify": "^6.0.0",
    "formik": "^2.4.5",
    "inversify": "^6.0.2",
    "inversify-react": "^1.1.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "moment": "^2.30.1",
    "nanobus": "^4.5.0",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "reflect-metadata": "^0.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mobx-devtools/tools": "^0.0.4",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-svgr": "^4.2.0"
  }

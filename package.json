{
  "name": "gorilla-vault",
  "version": "0.0.1",
  "description": "This is gorilla vault secure storage solution",
  "author": "Giorgi Kumelashvili",
  "private": "false",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "build:run": "vite preview",
    "lint": "npm run use:nvm && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "modules:update:check": "npm run use:nvm && npx npm-check-updates",
    "modules:update": "npm run use:nvm && npx npm-check-updates -u",
    "use:nvm": "source ~/.nvm/nvm.sh && nvm use",
    "install": "npm run use:nvm",
    "preinstall": "npm run use:nvm",
    "prebuild": "npm run use:nvm",
    "preversion": "npm run use:nvm",
    "prestart": "npm run use:nvm"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.2.0",
    "@blueprintjs/datetime2": "^2.2.1",
    "@blueprintjs/select": "^5.0.16",
    "@blueprintjs/table": "^5.0.8",
    "@total-typescript/ts-reset": "^0.4.2",
    "axios": "^1.4.0",
    "crypto-js": "^4.2.0",
    "formik": "^2.4.3",
    "inversify": "^6.0.1",
    "inversify-react": "^1.1.0",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.5",
    "moment": "^2.29.4",
    "nanobus": "^4.5.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.15.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "sass": "^1.65.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.10",
    "vite-plugin-checker": "^0.6.2"
  }
